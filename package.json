{
  "name": "sermo-node-test",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf build",
    "dist": "npm run clean && npm run build",
    "start": "node -r dotenv/config build/index.js",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "lint": "npm run lint:misc && npm run lint:scripts",
    "lint:fix": "npm run lint:misc:fix && npm run lint:scripts:fix",
    "lint:misc": "prettier --check \"**/*.{json,y?(a)ml,md}\"",
    "lint:misc:fix": "prettier --write \"**/*.{json,y?(a)ml,md}\"",
    "lint:scripts": "eslint \"**/*.@(j|t)s?(x)\" && tsc --noEmit",
    "lint:scripts:fix": "eslint --fix \"**/*.@(j|t)s?(x)\" && tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "http-status-codes": "2.1.4",
    "node-fetch": "2.6.5"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/nock": "11.1.0",
    "@types/node": "16.11.6",
    "@types/node-fetch": "2.5.12",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.3.1",
    "jest-ts-webcompat-resolver": "1.0.0",
    "nock": "13.1.4",
    "prettier": "2.4.1",
    "supertest": "6.1.6",
    "typescript": "4.4.4"
  }
}
